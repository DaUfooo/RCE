<script language="VBScript">
Sub window_onload()
  result = MsgBox("In einem echten Angriff haette sich nicht nur die CMD, die PowerShell oder der Calc geoeffnet. Sondern eine Schadsoftware!", 48, "Security Awareness")
  CreateObject("WScript.Shell").Run "cmd"
  CreateObject("WScript.Shell").Run "powershell"
  CreateObject("WScript.Shell").Run "calc"
End Sub
</script>
